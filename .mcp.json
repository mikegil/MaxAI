{
    "mcpServers": {
        "context7": {
            "type": "http",
            "description": "Up to date API info",
            "url": "https://mcp.context7.com/mcp",
            "headers": {
                "x-api-key": "REDACTED_API_KEY"
            }
        },
        "playwright": {
            "command": "docker",
            "args": [
                "run",
                "-i",
                "--rm",
                "mcp/playwright"
            ]
        },
        "MCP_DOCKER": {
            "command": "docker",
            "args": [
                "mcp",
                "gateway",
                "run"
            ],
            "type": "stdio"
        }
    }
}